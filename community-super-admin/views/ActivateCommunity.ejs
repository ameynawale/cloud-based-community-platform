<body>
<div id="wrapper">
    <nav class="navbar navbar-default top-navbar" role="navigation">
        <div class="navbar-header">
            <a class="navbar-brand" href="index.html"><strong>Communities</strong></a>

        </div>

        <ul class="nav navbar-top-links navbar-right">
            <!-- /.dropdown -->
            <li class="dropdown">
                <a class="dropdown-toggle" data-toggle="dropdown" href="#" aria-expanded="false">
                    <i class="fa fa-user fa-fw"></i> <i class="fa fa-caret-down"></i>
                </a>
                <ul class="dropdown-menu dropdown-user">
                    <li><a href="SignUp"><i class="fa fa-user fa-fw"></i>Register</a>
                    </li>
                    <li><a href="signin"><i class="fa fa-gear fa-fw"></i>Sign In</a>
                    </li>
                </ul>
                <!-- /.dropdown-user -->
            </li>
            <!-- /.dropdown -->
        </ul>
    </nav>
    <!--/. NAV TOP  -->
    <nav class="navbar-default navbar-side" role="navigation">
        <div class="sidebar-collapse">
            <ul class="nav" id="main-menu">

                <li>
                    <a href="index.html"><i class="fa fa-dashboard"></i> Communities</a>
                </li>
                <li>
                    <a href="index.html"><i class="fa fa-dashboard"></i> New Community Requests</a>
                </li>
            </ul>

        </div>

    </nav>
    <!-- /. NAV SIDE  -->
    <div id="page-wrapper" >


        <div id="page-inner">


            <div class="col-md-12 col-sm-12">
                <form class="col-md-12 col-sm-12" role="form" method="post" action="activatedSuccessfully">

                        <div class="col-md-8 col-sm-8">
                            <div class="panel panel-default">
                                <div>
                                    <label class="col-sm-4 col-md-4 col-lg-4">Community:</label>
                                    <div>
                                        <%=data[0].community%>
                                    </div>
                                </div>
                                <br/>
                                <div>
                                    <label class="col-sm-4 col-md-4 col-lg-4">Community Purpose:</label>
                                    <div>
                                        <%=data[0].purpose%>
                                    </div>
                                </div>
                                <br/>
                                <div class="form-group">
                                    <div
                                            class="col-xs-offset-4 col-sm-offset-4 col-md-offset-4 col-lg-offset-4">
                                        <button type="submit" class="btn btn-primary">Activate</button>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <div class="col-sm-8 col-md-8 col-lg-8">
                                        <input type="hidden" class="form-control" name="community"
                                               id="community" value="<%=data[0].community%>">
                                    </div>
                                </div>

                            </div>
                        </div>


                </form>
            </div>
        </div>

    </div>
    <!-- /. PAGE INNER  -->
</div>
